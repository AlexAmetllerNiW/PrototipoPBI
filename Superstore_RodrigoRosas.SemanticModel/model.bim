          },
          {
            "name": "Total Profit",
            "annotations": [
              {
                "name": "PBI_FormatHint",
                "value": "{\"currencyCulture\":\"en-US\"}"
              }
            ],
            "expression": "SUM(FACT_Orders[Profit])",
            "formatString": "\\$#,0.00;(\\$#,0.00);\\$#,0.00",
            "lineageTag": "c3c0e00d-9f73-444f-b3f8-769b7026a048"
          },
          {
            "name": "Average Order Value",
            "expression": "DIVIDE(SUM('FACT_Orders'[Sales]), COUNT('FACT_Orders'[Order ID]))",
            "formatString": "\\$#,0.00;(\\$#,0.00);\\$#,0.00",
            "lineageTag": "97f881a4-da64-4863-99de-cdc0745b528f"
          },
          {
            "name": "Total Customers",
            "expression": "DISTINCTCOUNT('FACT_Orders'[Customer ID])",
            "formatString": "0",
            "lineageTag": "4a816657-3183-467b-b763-3e0cdcd9d493"
          },
          {
            "name": "Return Rate",
            "expression": "DIVIDE(COUNT('DIM_Returns'[Order ID]), COUNT('FACT_Orders'[Order ID]), 0)",
            "formatString": "0.00%;-0.00%;0.00%",
            "lineageTag": "98527d2e-21ce-41da-97c3-2f29d8be3771"
          },
          {
            "name": "Current Year Sales",
            "expression": "CALCULATE(SUM('FACT_Orders'[Sales]), YEAR('FACT_Orders'[Order Date]) = YEAR(MAX('FACT_Orders'[Order Date])))",
            "formatString": "\\$#,0.00;(\\$#,0.00);\\$#,0.00",
            "lineageTag": "b0244daa-db1a-4ed1-9120-8c9d1b968f32"
          },
          {
            "name": "Previous Year Sales",
            "annotations": [
              {
                "name": "PBI_FormatHint",
                "value": "{\"isGeneralNumber\":true}"
              }
            ],
            "expression": [
              "",
              "VAR SelectedYear = MAX('FACT_Orders'[Order Date].[Year])",
              "VAR Previous = SelectedYear - 1",
              "VAR PreviousYearSales = CALCULATE(SUM('FACT_Orders'[Sales]), 'FACT_Orders'[Order Date].[Year] = Previous)",
              "RETURN",
              "    IF(ISBLANK(PreviousYearSales), BLANK(), PreviousYearSales)",
              ""
            ],
            "lineageTag": "bb832ff9-cb5c-49c3-82c8-7f10b6ecb4f1"
          },
          {
            "name": "Average Sales per Unit",
            "annotations": [
              {
                "name": "PBI_FormatHint",
                "value": "{\"currencyCulture\":\"en-US\"}"
              }
            ],
            "expression": "DIVIDE(SUM('FACT_Orders'[Sales]), SUM('FACT_Orders'[Quantity]))",
            "formatString": "\\$#,0.00;(\\$#,0.00);\\$#,0.00",
            "lineageTag": "7f21d72f-c532-4436-b1a7-0e5bcd590543"
          },
          {
            "name": "YoY Sales Growth%",
            "expression": [
              "",
              "VAR CurrentYearSales = [Current Year Sales]",
              "VAR PreviousYearSales = [Previous Year Sales]",
              "VAR GrowthPercentage = DIVIDE(CurrentYearSales - PreviousYearSales, PreviousYearSales)",
              "RETURN",
              "    IF(",
              "        GrowthPercentage > 0,",
              "        IF(",
              "            ISBLANK(PreviousYearSales),",
              "            BLANK(),",
              "            FORMAT(GrowthPercentage, \"0.00%\") & \" \" & UNICHAR(9650) -- Green triangle symbol (▲)",
              "        ),",
              "        IF(",
              "            GrowthPercentage < 0,",
              "            IF(",
              "                ISBLANK(PreviousYearSales),",
              "                BLANK(),",
              "                FORMAT(GrowthPercentage, \"0.00%\") & \" \" & UNICHAR(9660) -- Red inverted triangle symbol (▼)",
              "            ),",
              "            FORMAT(GrowthPercentage, \"0.00%\") -- No symbol for zero growth",
              "        )",
              "    )",
              ""
            ],
            "lineageTag": "9b3551b0-4d9c-49be-a857-fc9b2b77560f"
          },
          {
            "name": "Current Year Profit",
            "expression": "CALCULATE(SUM('FACT_Orders'[Profit]), YEAR('FACT_Orders'[Order Date]) = YEAR(MAX('FACT_Orders'[Order Date])))",
            "formatString": "\\$#,0.00;(\\$#,0.00);\\$#,0.00",
            "lineageTag": "4e952ad2-8066-48f2-80b8-4a092e8679de"
          },
          {
            "name": "Previous Year Profit",
            "expression": [
              "",
              "VAR SelectedYear = MAX('FACT_Orders'[Order Date].[Year])",
              "VAR Previous = SelectedYear - 1",
              "VAR PreviousYearProfit = CALCULATE(SUM('FACT_Orders'[Profit]), 'FACT_Orders'[Order Date].[Year] = Previous)",
              "RETURN",
              "    IF(ISBLANK(PreviousYearProfit), BLANK(), PreviousYearProfit)",
