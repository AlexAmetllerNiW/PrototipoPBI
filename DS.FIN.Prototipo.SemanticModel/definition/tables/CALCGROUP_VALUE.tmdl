/// DW: PBI.CALCGROUP_VALUE
table CALCGROUP_VALUE
	lineageTag: f9b086a1-30ca-411b-bc8a-dceecd14e294

	calculationGroup
		precedence: 2

		calculationItem Value = SELECTEDMEASURE()

		calculationItem (%) =
				
				SWITCH(
				    SELECTEDMEASURENAME(),
				
				    // Local Currency
				    "PL Balance", [PL Balance (%)],
				    "PL Budget", [PL Budget (%)],
				    "PL Budget Admin", [PL Budget Admin (%)],
				    "Deviation N/N-1", [Deviation N/N-1 (%)],
				    "Deviation N/N Budget", [Deviation N/N Budget (%)],
				    "Deviation N/N Budget Admin", [Deviation N/N Budget Admin (%)],
				    "Last Year", [Last Year (%)],
				
				    // EUR
				    "PL Balance (EUR)", [PL Balance (EUR) (%)],
				    "PL Budget (EUR)", [PL Budget (EUR) (%)],
				    "PL Budget Admin (EUR)", [PL Budget Admin (EUR) (%)],
				    "Deviation N/N-1 (EUR)", [Deviation N/N-1 (EUR) (%)],
				    "Deviation N/N Budget (EUR)", [Deviation N/N Budget (EUR) (%)],
				    "Deviation N/N Budget Admin (EUR)", [Deviation N/N Budget Admin (EUR) (%)],
				    "Last Year (EUR)", [Last Year (EUR) (%)],
				
				    // UI
				    "Pl Display Order", [Pl Display Order],
				    "Last Refresh", [Last Refresh],
				    "Titulo", [Visual Title]
				)

			formatStringDefinition = "0.00%"

